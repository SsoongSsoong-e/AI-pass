{
  "name": "visionITssu",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "docker-compose -p ai-pass-dev -f docker-compose.dev.yml up -d postgres mongodb && turbo dev",
    "dev:down": "docker-compose -p ai-pass-dev -f docker-compose.dev.yml down",
    "dev:full": "docker-compose -p ai-pass-dev -f docker-compose.dev.yml up -d --build",
    "dev:full:logs": "docker-compose -p ai-pass-dev -f docker-compose.dev.yml logs -f",
    "dev:full:stop": "docker-compose -p ai-pass-dev -f docker-compose.dev.yml down",
    "dev:ssl": "DOMAIN=localhost ./scripts/generate-ssl.sh",
    "prod": "docker-compose -p ai-pass-prod up -d",
    "prod:down": "docker-compose -p ai-pass-prod down",
    "prod:build": "npm run build && docker-compose -p ai-pass-prod up -d --build",
    "prod:logs": "docker-compose -p ai-pass-prod logs -f",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "start:debug": "nest start --debug --watch"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "^2.2.3",
    "typescript": "5.5.4"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.2.3",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@tailwindcss/vite": "^4.1.17",
    "@types/styled-components": "^5.1.34",
    "chalk": "^5.3.0",
    "path-scurry": "^2.0.0",
    "socket.io-client": "^4.8.1",
    "styled-components": "^6.1.13",
    "tailwindcss": "^4.1.17",
    "vite": "^6.0.2",
    "vite-plugin-svgr": "^4.3.0"
  }
}
